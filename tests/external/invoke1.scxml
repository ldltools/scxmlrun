<?xml version="1.0" encoding="utf-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0" datamodel="ecmascript">

  <state id="q1">
    <invoke id="hello" type="http://www.w3.org/TR/scxml/">
      <content>
	<scxml version="1.0">
	  <final id="hello">
	    <onentry><log label="log" expr="hello"/></onentry>
	  </final>
	</scxml>
      </content>
    </invoke>
    <transition event="done.invoke.hello" target="q2"/>
  </state>

  <final id="q2">
    <onentry><script>console.log ("done")</script></onentry>
  </final>

</scxml>
